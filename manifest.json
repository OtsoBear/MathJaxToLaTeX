{
  "manifest_version": 3,
  "name": "MathJax Copy Tool",
  "version": "2.5",
  "description": "Copy LaTeX from MathJax elements by clicking on them",
  "permissions": ["activeTab", "clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "config/operators.js",
        "config/functions.js",
        "modules/logger.js",
        "modules/node-processor.js",
        "modules/svg-converter.js",
        "modules/chtml-converter.js",
        "fileunicode.js",
        "translate.js",
        "content.js",
        "popup.js"
      ],
      "css": ["styles.css"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    "48": "icon.png"
    }
  },
  "icons": {
    "48": "icon.png"
  }
}
